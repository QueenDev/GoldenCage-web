{%extends "GoldenCageBackOfficeBundle::layoutadmin.html.twig"%}
{%block pieData%}
    {% for c in traiter %}
        {% for d in non_traiter %}
       
		var pieData = [

                                {
                                        value:{{ c.adm}},
                                        color:"#008000"
                                },

                                {
                                        value :{{ d.adm1}},
                                        color : "#FF0000"
                                }



                ];
                    
        {% endfor %}
    {% endfor %}
    var myPie = new Chart(document.getElementById("pie-chart").getContext("2d")).Pie(pieData);
{%endblock%}
                
                
                
{%block Content%}
<div class="main-inner">
            <div class="container">
                <div class="row">
                    <div class="span6">
                        <div class="widget">
                            <div class="widget-header">
                                <i class="icon-bar-chart"></i>
                                <h3>
                                    Statistique sur les réclamations</h3>
                            </div>
                            <!-- /widget-header -->
                            <div class="widget-content">
                                <canvas id="pie-chart" class="chart-holder" width="538" height="250">
                                </canvas>
                                {% for c in traiter %}
                                    {% for d in non_traiter %}
                                        <h4 style="color:#008000 "> ---- &nbsp;&nbsp; {{c.adm}} traiter &nbsp;&nbsp;&nbsp;&nbsp;  </h4> 
                                    {% endfor %}
                                {% endfor %}
                                {% for c in traiter %}
                                    {% for d in non_traiter %}
                                        <h4 style="color:#FF0000 "> ---- &nbsp;&nbsp; {{d.adm1}} non traiter &nbsp;&nbsp;&nbsp;&nbsp;    </h4>
                                    {% endfor %}
                                {% endfor %}
                                <!-- /pie-chart -->
                            </div>
                            <!-- /widget-content -->
                        </div>
                        <!-- /widget -->
                        <div class="widget">
                            <div class="widget-header">
                                <i class="icon-bar-chart"></i>
                                <h3>
                                    Statistique sur les services</h3>
                            </div>
                            <!-- /widget-header -->
                            <div class="widget-content">
                                <canvas id="bar-chart" class="chart-holder" width="538" height="250">
                                </canvas>
                                <!-- /bar-chart -->
                            </div>
                            <!-- /widget-content -->
                        </div>
                        <!-- /widget -->
                        <div class="widget">
                            <div class="widget-header">
                                <i class="icon-bar-chart"></i>
